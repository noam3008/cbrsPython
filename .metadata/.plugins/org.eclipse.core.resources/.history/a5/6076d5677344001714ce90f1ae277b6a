import pytest
from allure import pytest_plugin


def capital_case(x):
    return x.capitalize()

def test_capital_case():
    assert capital_case('semaphore') == 'Semaphore'
    
@pytest_plugin.StepContext
def make_test_data_foo():
    name = "noam"
    assert name=="noam"
    
    


test_capital_case()
make_test_data_foo()
