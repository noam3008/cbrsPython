import time
from threading import Thread
from thread import start_new_thread

def prints():
    print "hello"
    
def myfunc(i):
    print "sleeping 5 sec from thread %d" % i
    time.sleep(5)
    print "finished sleeping from thread %d" % i

for i in range(10):
    t = Thread(target=myfunc, args=(i,))
    t.start()
    start_new_thread(prints,i)