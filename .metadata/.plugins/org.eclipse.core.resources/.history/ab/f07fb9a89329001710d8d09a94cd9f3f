from flask import Flask
class flaskExample(object):
    '''
    classdocs
    '''
    app = Flask(__name__)

    def __init__(self):
        '''
        Constructor
        '''
        self.app = Flask(__name__)
        self.app.run()
    
    @app.route('/') 
    def helloWorld(self):
        print "djmd"
        return "Hello World!asdasd"
        
flaskExamp = flaskExample()