from flask import Flask
class FlaskAppWrapper(object):
    app = Flask(__name__)

    def __init__(self, name):
        self.app = Flask(name)

    def run(self):
        print ("run flask server")
        if __name__ == '__main__':
            self.app.run(host="10.3.35.58",port="5000",threaded=True)
        
    @app.route('/')
    def getApi(self):
        return "hello"
