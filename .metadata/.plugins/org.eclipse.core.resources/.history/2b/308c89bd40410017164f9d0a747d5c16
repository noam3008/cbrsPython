from Tkinter import *
from model.Utils import Consts as consts

root = Tk()

topFrame = Frame(root)
topFrame.pack()
bottomFrame = Frame(root)
bottomFrame.pack(side=BOTTOM)
typeCsvFileLabel = Label(topFrame,text=consts.SET_CSV_FILE_MESSAGE)
typeCsvFileLabel.pack()

csvFileSendButton = Button(topFrame,text = "submit",fg = "red")
csvFileSendButton2 = Button(topFrame,text = "submit",fg = "purple")
csvFileSendButton3 = Button(topFrame,text = "submit",fg = "yellow")
csvFileSendButton4 = Button(topFrame,text = "submit",fg = "blue")

csvFileSendButton.pack()
csvFileSendButton2.pack(side=LEFT)
csvFileSendButton3.pack()
csvFileSendButton4.pack()
root.mainloop()

        