from Tkinter import *
from model.Utils import Consts as consts

root = Tk()
def print_Left(event):
    print "left"
def print_Right(event):
    print "rigth"
def print_Middle(event):
    print "middle"    

frame = Frame(root,width=300,height=200)
frame.bind("<Button-1>", print_Left)
frame.bind("<Button-2>", print_Middle)
frame.bind("<Button-3>", print_Right)           

root.mainloop()

        