class TestDefinition(object):
    '''
    classdocs
    '''


    def __init__(self, steps):
        '''
        Constructor
        '''
        self.defenitionsOfSteps = []
        self.jsonNamesOfSteps = []
        for step in steps:    
            self.defenitionsOfSteps.append(step.defOfStep)
            self.jsonNamesOfSteps.append(step.jsonOfStep)