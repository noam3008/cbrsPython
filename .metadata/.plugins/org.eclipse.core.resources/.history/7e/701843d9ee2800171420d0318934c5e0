from flask import Flask
class FlaskAppWrapper(object):
    app = None

    def __init__(self, name):
        self.app = Flask(name)

    def run(self):
        print ("run flask server")
        self.app.run(host="10.3.35.58",threaded=True)
        
    @app.route('/')
    def getApi(self):
        return "hello"
