from Tkinter import *
from model.Utils import Consts as consts

root = Tk()
typeCsvFileLabel = Label(root,text="name",bg="red",fg="white")
typeCsvFileLabel2 = Label(root,text="password",bg="red",fg="white")

entry1 = Entry(root)
entry2 = Entry(root)
typeCsvFileLabel.grid(row=0,sticky =LEFT)
typeCsvFileLabel2.grid(row=1)
entry1.grid(row=0,column=1)
entry2.grid(row=1,column=1)


root.mainloop()

        