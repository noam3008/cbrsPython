from abc import abstractmethod
 
class Observer(object):
    
    def __init__(self,dir_path):
        self.dir_path = dir_path
        
 
    @abstractmethod
    def startTest(self,log_Name,folder_Name=None):
        pass
    
    @abstractmethod
    def startStep(self):
        pass
        
    @abstractmethod    
    def finishStep(self):
        pass
        
    @abstractmethod   
    def finishTest(self):
        pass