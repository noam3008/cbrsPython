from model.Engine import MyEngine
import logging
class ENodeBController(object):
    '''
    classdocs
    '''


    def __init__(self):
        '''
        Constructor
        '''
        self.engine = MyEngine()
        
    def sentRequestToEngine(self):
        ##self.engine.process_request(httpRequest)
        logging.info("the enode B controller sent the request he gets from the client to the engine")
        print self.engine.jsonFileName
        self.engine.process_request(self.engine.process_request("grant.json"))
    
    def sentResponseToClient(self,responseFromEngine):
        logging.info("the enodeB controller sent the engine response " + responseFromEngine + " to the client")
        return responseFromEngine
        
        