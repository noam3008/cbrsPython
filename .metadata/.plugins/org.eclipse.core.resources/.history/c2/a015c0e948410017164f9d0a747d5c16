from Tkinter import *
from model.Utils import Consts as consts
    
mainFrame = Tk()

mainFrame.option_add("*Button.Background", "black")
mainFrame.option_add("*Button.Foreground", "red")

mainFrame.title('The game')
mainFrame.geometry("500x500")
mainFrame.resizable(0, 0)

openFrame = Frame(mainFrame,size=LEFT)




mainFrame.mainloop()       