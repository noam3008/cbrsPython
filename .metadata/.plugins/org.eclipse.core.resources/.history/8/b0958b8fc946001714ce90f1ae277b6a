from Loggers.logObserver import Observer
 
class xmlLogger(Observer):
    def startTest(self, *args, **kwargs):
        print("European stock market received: {0}\n{1}".format(args, kwargs))
 
    def startStep(self):
        print "startStep xml"
        
        
    def finishStep(self):
        print "finishStep xml"
        
        
    def finishTest(self):
        print "finish test xml"