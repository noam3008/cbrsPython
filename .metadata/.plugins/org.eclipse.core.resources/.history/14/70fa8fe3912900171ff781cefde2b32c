from flask import Flask
class flaskExample(object):
    '''
    classdocs
    '''
    app = Flask(__name__)

    def __init__(self, params):
        '''
        Constructor
        '''
        self.app = Flask(__name__)
        self.app.run()
    
    @app.route('/') 
    def helloWorld(self):
        return "Hello World!"
        