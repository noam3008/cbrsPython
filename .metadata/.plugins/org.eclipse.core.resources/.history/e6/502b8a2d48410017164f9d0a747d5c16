from Tkinter import *
from model.Utils import Consts as consts

root = Tk()

def get_Selection():
    print "lkalal"
    

    
variable = StringVar(root)
variable.set("one") # default value
option = OptionMenu(root,variable,"one","two","three")
option.pack()

def ok():
    print "value is", variable.get()
    root.quit()

button = Button(root, text="OK", command=ok)
button.pack()

mainloop()
       

root.mainloop()

        